{
  "type": "minecraft:notice",
  "title": {
    "translate": "dialog.name.title"
  },
  "body": {
    "type": "minecraft:plain_message",
    "contents": {
      "translate": "dialog.name.discription"
    }
  },
  "inputs": [
    {
      "type": "minecraft:text",
      "key": "name",
      "label": {
        "translate": "dialog.name.text"
      }
    }
  ],
  "can_close_with_escape": false,
  "action": {
    "label": {
      "translate": "dialog.name.button.ok"
    },
    "tooltip": {
      "translate": "dialog.name.button.ok.discription"
    },
    "action": {
      "type": "dynamic/run_command",
      "template": "function core:regist_name_macro {name:\"$(name)\"}"
    }
  }
}